@using Technico.Session
@using TechnicoWebApi.Models
@using static TechnicoWebApi.Models.OwnerType

@{
    Layout = "_Layout";
    var ownerType = SessionClass.ownerType;

        if (ownerType == OwnerType.Owner)
        {
            Layout = "_UserLayout";
        }
        else
        {
            Layout = "_AdminLayout";
        }
    
    
         
}
